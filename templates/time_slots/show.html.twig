{% extends 'base.html.twig' %}

{% block title %}TimeSlots{% endblock %}

{% block body %}
    <h1>TimeSlots</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ time_slot.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ time_slot.name }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ time_slot.date ? time_slot.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>FromTime</th>
                <td>{{ time_slot.fromTime ? time_slot.fromTime|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ToTime</th>
                <td>{{ time_slot.toTime ? time_slot.toTime|date('H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_time_slots_index') }}">back to list</a>

    <a href="{{ path('app_time_slots_edit', {'id': time_slot.id}) }}">edit</a>

    {{ include('time_slots/_delete_form.html.twig') }}
{% endblock %}
